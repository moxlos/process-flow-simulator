        {"id": "Patient-05", "tasks": ["Triage", "Registration", "Examination", "Treatment", "Discharge"]},
        {"id": "Patient-06", "tasks": ["Triage", "Registration", "Examination", "Treatment", "Discharge"]},
        {"id": "Patient-07", "tasks": ["Triage", "Registration", "Examination", "Treatment", "Discharge"]},
        {"id": "Patient-08", "tasks": ["Triage", "Registration", "Examination", "Treatment", "Discharge"]}
    ]
}
